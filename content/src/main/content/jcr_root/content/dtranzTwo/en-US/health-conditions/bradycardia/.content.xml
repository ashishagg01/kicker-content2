                jcr:primaryType="nt:unstructured"
                sling:resourceType="dtranzTwo/components/content/subtitle"
                selectType="h2"
                text="Treatment"/>
            <text_3
                jcr:created="{Date}2015-09-18T11:15:33.168+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-10-05T14:45:18.393+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dtranzTwo/components/content/text"
                richTxtEditor="&lt;p>&lt;p>&lt;b>How is bradycardia treated?&lt;/b>&lt;br>&#xa;Doctors may prescribe medications, a pacemaker, or both for the treatment of bradycardia.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;/p>&#xa;&lt;p>&lt;a href=&quot;http://www.bostonscientific.com/en-US/health-conditions/bradycardia/Treatment.html&quot;>Learn More about Treatments for Bradycardia&amp;nbsp;&lt;/a>&lt;/p>&#xa;"/>
