                jcr:lastModified="{Date}2015-09-18T11:12:16.311+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dtranzTwo/components/content/subtitle"
                selectType="h2"
                text="Symptoms"/>
            <text_1
                jcr:created="{Date}2015-09-18T11:13:36.275+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-10-05T14:44:34.903+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dtranzTwo/components/content/text"
                richTxtEditor="&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&lt;b>What are the symptoms of bradycardia?&lt;/b>&lt;br>&#xa;Someone with bradycardia may feel tired, light-headed, or dizzy.&lt;/p>&#xa;&lt;p>&lt;a href=&quot;http://www.bostonscientific.com/en-US/health-conditions/bradycardia/Symptoms.html&quot;>Learn More about Symptoms of Bradycardia&amp;nbsp;&lt;/a>&lt;/p>&#xa;&lt;div>&amp;nbsp;&lt;/div>&#xa;"/>
            <subtitle_2
